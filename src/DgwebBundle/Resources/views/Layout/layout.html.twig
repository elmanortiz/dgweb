            <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
            <script src="{{asset('https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js')}}"></script>
            <!-- Include all compiled plugins (below), or include individual files as needed -->
            <script src="{{asset('Resources/js/bootstrap.min.js')}}"></script>
            <!-- Javascript Archivos -->
            <script src="{{asset('Resources/js/scrollReveal.js')}}"></script>
            <script>
                window.sr = new scrollReveal();
            </script>
            <!-- Find Javascript Archivos -->
            <script type="text/javascript">
                $(document).scroll(function () {
                    var y = $(this).scrollTop();
                    if (y > 200) {
                        $('.bottomMenu').fadeIn();
                    } else {
                        $('.bottomMenu').fadeOut();
                    }
                });
            </script>
            

            <script src="{{ asset('Resources/js/typed.js')}}" type="text/javascript"></script>
            {#<script type="text/javascript" src="{{ asset('Resources/jquery/jquery_1.11.3.js')}}"></script>#}
            <script>
            $(function () {
                $(".element").typed({
                    strings: ["<strong>creative</strong>", "<strong>developers</strong>", "<strong>designers</strong>", "<strong>digitality garage.</strong>"],
                    startDelay: 2000,
                    typeSpeed: 100,
                    backDelay: 1000,
                    loop: false,
                    contentType: 'html', // or text
                    // defaults to false for infinite loop
                    loopCount: false,
                    callback: function () {
                        foo();
                    },
                    resetCallback: function () {
                        newTyped();
                    }
                });

                $(".reset").click(function () {
                    $("#typed").typed('reset');
                });

            });

            function newTyped() { /* A new typed object */
            }

            function foo() {
                console.log("Callback");
            }

            </script>
