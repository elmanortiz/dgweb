{% extends 'DgwebBundle:Layout:layout.html.twig' %}

{% block body -%}
  

    <div class="container-fluid" style="background-color: #F6F6F6;">
         <h2 class="text-center" style="color: #828282; margin-top: 70px; font-family: 'Montserrat', sans-serif; font-weight: 400; font-size: 18px;">PROYECTOS</h2>
        <div style="width: 60px; border-top: 2px solid #09F; margin-left: auto; margin-right: auto; margin-top: 10px;"></div></h2>     
        
	<!-- Footer Desktop -->
	<div class="container" style="background-color: #F6F6F6; padding-top:60px; padding-bottom: 30px;">
          <div class="row">


                <table class="records_list">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Titulo</th>
                            <th>Contenido</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody>
                    {% for entity in entities %}
                        <tr>
                            <td><a href="{{ path('admin_proyecto_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                            <td>{{ entity.titulo }}</td>
                            <td>{{ entity.contenido }}</td>
                            <td>
                            <ul>
                                <li>
                                    <a href="{{ path('admin_proyecto_show', { 'id': entity.id }) }}">show</a>
                                </li>
                                <li>
                                    <a href="{{ path('admin_proyecto_edit', { 'id': entity.id }) }}">edit</a>
                                </li>
                            </ul>
                            </td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>

                    <ul>
                    <li>
                        <a href="{{ path('admin_proyecto_new') }}">
                            Create a new entry
                        </a>
                    </li>
                </ul>
                
                
  
            </div>
 </div>
	<!-- End Footer Desktop -->
</div>               
                
    {% endblock %}
